{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "builds": [
    { "src": "frontend/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "frontend/$1" }
  ]
}
{
  "version": 2,
  "builds": [
    { "src": "web/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/web/index.html" },
    { "src": "/(.*)", "dest": "/web/$1" }
  ]
}
